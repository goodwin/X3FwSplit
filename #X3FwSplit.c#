#include "X3FwSplit.h" 
#include <stdio.h>
#include <stdlib.h>


#include <fcntl.h>
#include <sys/types.h>
#include <sys/stat.h>

 

int Img2File (char *InputFilePath, char *OutputFilePath) 
{
   FILE *FwFile;
  /*
    Load .fw file and create memory image.
   */
  printf ("*Load %s ...", InputFilePath) ;
  FwFile = fopen (InputFilePath, "rb") ;
  if (FwFile == -1) {
    printf ("Load fw fail, EXIT\n") ;
    return -1;
  } else {
    printf ("OK.\n") ;
    fclose (FwFile) ;
  }
  return 0;
}
