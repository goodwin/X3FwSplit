#include "X3FwSplit.c" 

#ifdef LINUX
#include <stat.h>
#include <string.h> 
#endif 

int Img2File (char *InputFilePath, char *OutputFilePath) 
{
  FILE FwFile;
  /*
    Load .fw file and create memory image.
   */
  printf ("*Load %s ...", InputFilePath) ;
  FwFile = FileOpen (InputFilePath, "rw") ;
  if (FwFile == NULL) {
    printf ("Load fw fail, EXIT\n") ;
    return -1;
  }
  
  return 0;
}
